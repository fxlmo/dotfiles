{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"setup tcolorbox": {
		"prefix": "tcset",
		"body": [
			"\\newtcolorbox{blue}[3][]  {",
				"colframe = #2!25,",
				"colback = #2!10,",
				"#1,",
			"}",
			"\\newtcolorbox{blue}[3][] {",
				"colframe = #2!25,",
				"colback = #2!10,",
				"#1,",
			"}",
			"\\newtcolorbox{titlebox}[3][] {",
				"colframe = #2!25,",
				"colback = #2!10,",
				"coltitle = #2!20!black,",
				"title = {#3},",
				"fonttitle=\\bfseries",
				"#1,",
			"}"
		],
		"description": "setup tcolorbox with all the necessary things"
	},
	"bold text": {
		"prefix":"bf",
		"body": [
			"\\textbf{$1}$0"
		]
	},
	"italic text": {
		"prefix":"ita",
		"body": [
			"\\textit{$1}$0"
		]
	},
	"typetext": {
		"prefix":"tt",
		"body": [
			"\\texttt{$1}$0"
		]
	},
	"part": {
		"prefix":"par",
		"body": [
			"\\part{$1}\n$0"
		]
	},
	"chapter": {
		"prefix":"cha",
		"body": [
			"\\chapter{$1}\n$0"
		]
	},
	"section":{
		"prefix":"sec",
		"body": [
			"\\section{$1}\n$0"
		]
	},
	"subsection":{
		"prefix":"sub",
		"body": [
			"\\subsection{$1}\n$0"
		]
	},
	"subsubsection": {
		"prefix":"ssub",
		"body": [
			"\\subsubsection{$1}\n$0"
		]
	},
	"center environment": {
		"prefix": "center",
		"body": [
			"\\begin{center}",
			"\t$1",
			"\\end{center}"
		]
	},
	"table environment": {
		"prefix": "tab",
		"body": [
			"\\begin{tabular}{$1}",
			"\t$2",
			"\\end{tabular}$0"
		]
	},
	"set table rowcolours": {
		"prefix": "rc",
		"body": [
			"\\rowcolors{2}{gray!25}{white}"
		]
	},
	"mathmode": {
		"prefix" : "ma",
		"body" : [
			"$$1$$0"
		]
	},
	"color": {
		"prefix":"col",
		"body": [
			"{\\color{${1:red}}$2}$0"
		]
	},
	"tcolorbox with title": {
		"prefix":"tcbtit",
		"body": [
			"\\begin{tcolorbox} [space to upper,",
					"collower=white,",
					"title={${1:title}},",
					"nobeforeafter,",
					"halign lower=flush right, ]",
			"$2",
			"\\end{tcolorbox}",
			"$0"
		]
	},

	"add image to document": {
		"prefix":"graphic",
		"body": [
			"\\includegraphics[${1:width=\\textwidth}]{${2:imagename}}"
		]
	},

	"minipage environment": {
		"prefix":"mini",
		"body": [
			"\\begin{minipage}{${1:1.0}\\textwidth}",
			"$2",
			"\\end{minipage}",
			"$0"
		]
	},

	"Favourite preamble": {
		"prefix":"doc",
		"body": [
			"% Preamble {{{",
			"\\documentclass[${1:11pt,a4paper}]{${2:scrartcl}}",
			"\\usepackage[english]{babel}",
			"\\typearea{${5:12}}",
			"% }}}",
			"",
			"% Set indentation and line skip for paragraph {{{",
			"\\setlength{\\parindent}{0em}",
			"\\setlength{\\parskip}{1em}",
			"\\usepackage[margin=2cm]{geometry}",
			"\\addtolength{\\textheight}{-1in}",
			"\\setlength{\\headsep}{.5in}",
			"% }}}",
			"",
			"% Headers setup {{{",
			"\\usepackage{${3:fancyhdr}}",
			"\\pagestyle{${4:fancy}}",
			"\\lhead{${6:title}}",
			"\\rhead{Josh Felmeden}",
			"% }}}",
			"",
			"\\begin{document}",
			"$0",
			"\\end{document}"
		]
	},

	"left arrow outside of math mode": {
		"prefix":"larrow",
		"body": [
			"$\\leftarrow$"
		]
	},

	"rightarrow outside of math mode": {
		"prefix":"rarrow",
		"body": [
			"$\\rightarrow$"
		]
	},

	"Multicolumn environment": {
		"prefix":"mulcol",
		"body": [
			"\\multicolumn{${1:1}}{${2:|c|}}{${3:text}}$0"
		]
	},

	"table title": {
		"prefix":"tabtit",
		"body": [
			"\\multicolumn{${1:number}}{${2:c}}{\\textbf{${3:title}}} \\\\",
			"\\multicolumn{$1}{c}{} \\\\ \\hline",
			"$0"
		]
	},

	"inline listing with language option": {
		"prefix":"lsil",
		"body": [
			"\\lstinline[language=${1:[Visual]Basic}]|$2|$0"
		]
	},

	"Setup lst preamble": {
		"prefix":"lstset",
		"body": [
			"% Listings {{{",
			"\\usepackage[]{listings}",
			"\\usepackage[]{xcolor}" ,
			"\\definecolor{RoyalBlue}{cmyk}{1, 0.50, 0, 0}",
			"\\lstset{language=${1:Language},",
				"keywordstyle=\\color{RoyalBlue},",
				"basicstyle=\\small\ttfamily,",
				"commentstyle=\\ttfamily\\itshape\\color{gray},",
				"stringstyle=\\ttfamily,",
				"showstringspaces=false,",
				"breaklines=true,",
				"frameround=ffff,",
				"frame=single,",
				"rulecolor=\\color{black}",
			"}",
			"% }}}"
		]
	},

	"Setup lst preamble with color": {
		"prefix":"lstcol",
		"body": [
			"% Listings {{{",
			"\\usepackage[]{listings,xcolor}" ,
			"\\lstset",
			"{",
				"breaklines=true,",
				"tabsize=3,",
				"showstringspaces=false",
			"}",
			"",
			"\\definecolor{lstgrey}{rgb}{0.05,0.05,0.05}",
			"\\usepackage{listings}",
			"\\makeatletter",
			"\\lstset{language=${1:[Visual]Basic},",
				"backgroundcolor=\\color{lstgrey},",
				"frame=single,",
				"xleftmargin=0.7cm,",
				"frame=tlbr, framesep=0.2cm, framerule=0pt,",
				"basicstyle=\\lst@ifdisplaystyle\\color{white}\\footnotesize\\ttfamily\\else\\color{black}\\footnotesize\\ttfamily\\fi,",
				"captionpos=b,",
				"tabsize=2,",
				"keywordstyle=\\color{Magenta}\\bfseries,",
				"identifierstyle=\\color{Cyan},",
				"stringstyle=\\color{Yellow},",
				"commentstyle=\\color{Gray}\\itshape",
			"}",
			"\\makeatother",
			"\\renewcommand{\\familydefault}{\\sfdefault}",
			"% }}}",
			"$0"
		]
	},

	"Setup title": {
		"prefix":"title",
		"body": [
			"% Title {{{",
			"\\author{Josh Felmeden}",
			"\\date{${1:month} \\\\ ${2:year}}",
			"\\title{${3:title}}",
			"% }}}",
			"${0}"
		]
	},

	"Insert bib preamble": {
		"prefix":"bib",
		"body": [
			"% Bibliography setup (in Harvard form) {{{",
			"\\usepackage[sorting=nyt, style=apa]{biblatex}",
			"\\usepackage{csquotes}",
			"\\usepackage{hyperref}",
			"\\sloppy",
			"\\bibliography{${7:bibliography}.bib}",
			"\\let\\cite\\parencite",
			"% }}}"
		]
	},

	"lstlisting change language": {
		"prefix":"lsl",
		"body": [
			"\\begin{lstlisting}[language=${1:[Visual]Basic}]",
			"${2}",
			"\\end{lstlisting}",
			"${0}"
		]
	},

	"lstlisting default language": {
		"prefix":"ls",
		"body": [
			"\\begin{lstlisting}",
			"${1}",
			"\\end{lstlisting}",
			"${0}"
		]
	},

	"cite": {
		"prefix":"ct",
		"body": [
			"~\\cite{${1}}$0"
		]
	},

	"align*": {
		"prefix": "al*",
		"body": [
			"\\begin{align*}",
				"${1}",
			"\\end{align*}",
			"${0}"
		]
	},

	"gather*": {
		"prefix": "gat*",
		"body": [
			"\\begin{gather*}",
				"${1}",
			"\\end{gather*}",
			"${0}"
		]
	},

	"Formal notes preamble": {
		"prefix": "formal",
		"body": [
			"% Preamble {{{",
			"\\documentclass[11pt,a4paper,titlepage,dvipsnames,cmyk]{scrartcl}",
			"\\usepackage[english]{babel}",
			"\\typearea{12}",
			"% }}}\n",
			"% Set indentation and line skip for paragraph {{{",
			"\\setlength{\\parindent}{0em}",
			"\\setlength{\\parskip}{1em}",
			"\\usepackage[margin=2cm]{geometry}",
			"\\addtolength{\\textheight}{-1in}",
			"\\setlength{\\headsep}{.5in}",
			"% }}}\n",
			"\\usepackage{hhline}" ,
			"\\usepackage[table]{xcolor}" ,
			"\\usepackage{mathtools}" ,
			"\\usepackage[T1]{fontenc}\n",
			"% Headers setup {{{",
			"\\usepackage{fancyhdr}",
			"\\pagestyle{fancy}",
			"\\lhead{$1}",
			"\\rhead{Josh Felmeden}",
			"\\usepackage{hyperref}" ,
			"% }}}\n",
			"% Listings {{{",
			"\\usepackage[]{listings}" ,
			"\\lstset",
			"{",
				"\tbreaklines=true,",
				"\ttabsize=3,",
				"\tshowstringspaces=false",
			"}\n",

			"\\definecolor{lstgrey}{rgb}{0.05,0.05,0.05}",
			"\\usepackage{listings}",
			"\\makeatletter",
			"\\lstset{language=[Visual]Basic,",
				"backgroundcolor=\\color{lstgrey},",
				"frame=single,",
				"xleftmargin=0.7cm,",
				"frame=tlbr, framesep=0.2cm, framerule=0pt,",
				"basicstyle=\\lst@ifdisplaystyle\\color{white}\\footnotesize\\ttfamily\\else\\color{black}\\footnotesize\\ttfamily\\fi,",
				"captionpos=b,",
				"tabsize=2,",
				"keywordstyle=\\color{Magenta}\\bfseries,",
				"identifierstyle=\\color{Cyan},",
				"stringstyle=\\color{Yellow},",
				"commentstyle=\\color{Gray}\\itshape",
			"}",
			"\\makeatother",
			"\\renewcommand{\\familydefault}{\\sfdefault}",
			"\\newcommand{\\specialcell}[2][c]{%",
			"\\begin{tabular}[#1]{@{}c@{}}#2\\end{tabular}}",
			"% }}}\n",

			"% Other packages {{{",
			"\\usepackage{graphicx}",
			"\\graphicspath{ {./pics/} }",
			"\\usepackage{needspace}",
			"\\usepackage{tcolorbox}",
			"\\usepackage{soul}",
			"\\usepackage{babel,dejavu,helvet}" ,
			"\\usepackage{amsmath}" ,
			"\\usepackage{booktabs}" ,
			"\\usepackage{tcolorbox}" ,
			"\\usepackage[symbol]{footmisc}" ,
			"\\renewcommand{\\thefootnote}{\\fnsymbol{footnote}}",
			"\\renewcommand{\\familydefault}{\\sfdefault}",
			"\\usepackage{enumitem}",
			"\\setlist{nolistsep}",
			"% }}}\n",

			"% tcolorbox {{{",
			"\\newtcolorbox{blue}[3][] {",
				"colframe = #2!25,",
				"colback = #2!10,",
				"#1,",
			"}\n",

			"\\newtcolorbox{titlebox}[3][] {",
				"colframe = #2!25,",
				"colback = #2!10,",
				"coltitle = #2!20!black,",
				"title = {#3},",
				"fonttitle=\\bfseries",
				"#1,",
			"}",
			"% }}}\n",

			"% Title {{{",
			"\\title{${1}}",
			"\\author{Josh Felmeden}",
			"% }}}\n",

			"\\begin{document}",
			"\\maketitle",
			"\\tableofcontents",
			"\\newpage",
			"${0}",
			"\\end{document}"
		]
	},

	"tcolorbox": {
		"prefix": "tcb",
		"body": [
			"\\begin{tcolorbox}",
				"${0}",
			"\\end{tcolorbox}"
		]
	},

	"Enumerate": {
		"prefix": "enum",
		"body": [
			"\\begin{enumerate}",
				"\\item $0",
			"\\end{enumerate}"
		]
	},

	"Itemize": {
		"prefix": "item",
		"body": [
			"\\begin{itemize}",
				"\t\\item $1",
			"\\end{itemize}",
			"$0"
		]
	},

	"Description": {
		"prefix": "desc",
		"body": [
			"\\begin{description}",
				"\t\\item[$1] $1",
			"\\end{description}",
			"$0"
		]
	},

	"Individual item": {
		"prefix": "it",
		"body": [
			"\\item $0"
		]
	},


	"Acroynm normal": {
		"prefix": "ac",
		"body": [
			"\\ac{${1:acronym}}",
			"$0"
		]
	},

	"Acroynm expanded": {
		"prefix": "acl",
		"body": [
			"\\acl{${1:acronym}}",
			"$0"
		]
	},

	"Non-indented paragraph": {
		"prefix": "ni",
		"body": [
			"\\noindent",
			"$0"
		]
	},

	"Long parenthesis": {
		"prefix": "lp",
		"body": [
			"\\left(${1:contents}\\right)$0"
		]
	},
	"Smart matrix environment": {
		"prefix": "mat",
		"body": [
			"\\begin{${1:p/b/v/V/B/small}matrix}",
				"\t${0}",
			"\\end{$2matrix}"
		]
	},
	"3x3 Matrix": {
		"prefix": "mat3",
		"body": [
			"\\begin{${1:p/b/v/V/B/small}matrix}",
				"\t$2 & $3 & $4 \\\\\\",
				"\t$5 & $6 & $7 \\\\\\",
				"\t$8 & $9 & $10",
			"\\end{$1matrix}"
		]
	}
}